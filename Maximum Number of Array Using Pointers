#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter %d elements:\n", n);
    for (int *p = arr; p < arr + n; p++) {
        scanf("%d", p);
    }

    int *max = arr; // assume first element is max
    for (int *p = arr + 1; p < arr + n; p++) {
        if (*p > *max)
            max = p;
    }

    printf("Maximum value = %d\n", *max);
    return 0;
}
