#include <stdio.h>

int main() {
    char str[100];
    int count = 0;
    char *p;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    for (p = str; *p != '\0'; p++) {
        if (*p=='a'||*p=='e'||*p=='i'||*p=='o'||*p=='u'||
            *p=='A'||*p=='E'||*p=='I'||*p=='O'||*p=='U')
            count++;
    }

    printf("Number of vowels = %d\n", count);
    return 0;
}
