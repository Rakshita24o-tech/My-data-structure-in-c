#include <stdio.h>

int main() {
    int arr[10] = {1, 2, 3, 5};
    int n = 4; 
    int pos = 3; 
    int newElement = 4;

    // shift elements to the right
    for (int i = n; i > pos; i--) {
        arr[i] = arr[i - 1];
    }

    arr[pos] = newElement;
    n++;

    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
}
