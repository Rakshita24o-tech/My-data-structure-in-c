#include <stdio.h>

int main() {
    int a, b, sum;
    int *p1, *p2;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    p1 = &a; // pointer to a
    p2 = &b; // pointer to b

    sum = *p1 + *p2; // dereference pointers to get values

    printf("Sum = %d\n", sum);

    return 0;
}
